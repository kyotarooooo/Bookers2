<div class="container">
	<div class="row sampleRow">
		<div class="col-xs-3">
			<%= render 'books/list2',user: @user %>
			<%= render 'books/list', book: @book %>
		</div>
		
	<div class="col-xs-9">
		<h1>Users</h1>
		<table class="table">
				<thead>
					<tr>
						<th>image</th>
						<th>name</th>
						<th></th>
					</tr>
				</thead>
			<% @users.each do |user| %>
				<tbody>
					<tr>
						<td><%= attachment_image_tag user, :profile_image, :fill, 100, 100, class: "user-image2", fallback: "noimage.png" %></td>
						<td><%= user.username %></td>
						<td><%= link_to "Show" , user_path(user) %></td>
					</tr>
				</tbody>
			<% end %>
		</table>
	</div>
	</div>
</div>
